{
    "name": "sroehrl/neoan3",
    "version": "3.1.0",
    "description": "the efficient PHP framework",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4||8.*",
        "ext-json": "*",
        "mnsami/composer-custom-directory-installer": "1.1.*",
        "neoan3-apps/template":"^1",
        "neoan3-apps/db": "0.3.*",
        "neoan3-apps/stateless": "^2",
        "neoan3-apps/session": "1.0.*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "neoan",
            "email": "neoan@neoan.us"
        }
    ],
    "autoload": {
        "psr-4": {
            "Neoan3\\Core\\": "./_neoan/base/",
            "Neoan3\\Api\\": "./_neoan/api/",
            "Neoan3\\Model\\": "./model/",
            "Neoan3\\Component\\": "./component/",
            "Neoan3\\Provider\\": "./provider/"

        },
        "files": [
            "_neoan/base/_includes.php"
        ]
    },
    "extra": {
        "installer-paths": {
            "./frame/{$name}": [],
            "./model/{$name}": [],
            "./component/{$name}": [],
            "./provider/{$name}": []
        }
    }
}
